{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown", "better-docs/component"],
  "templates": {
    "better-docs": {
      "name": "Event App"
    }
  },
  "opts": {
    "encoding": "utf-8",
    "destination": "docs",
    "recurse": true,
    "readme": "README.md",
    "verbose": true,
    "template": "./node_modules/better-docs"
  }
}
